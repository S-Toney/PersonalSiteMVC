<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="Hi, I'm Tessa! I’m a fill-stack developer working in ASP.net. I believe in teamwork, clear communication, and proactively meeting goals. I have the most experience in front-end development and am continuously improving in C# and SQL. " />
    <meta name="author" content="S. Tessa Toney" />
    <meta name="keywords" content="web development, projects, portfolio, examples, resume, history, education" />

    @*For the LinkedIn Feature Section Thumbnail*@
    <meta name="description" content="Crescendo App" />
    <meta name="title" property="og:title" content="Cescendo" />
    <meta property="og:type" content="Website" />
    <meta name="image" property="og:image" content="https://embedr.flickr.com/assets/client-code.js" />
    <meta name="description" property="og:description" content="Crescendo App" />
    <meta name="author" content="S. Tessa Toney" />

    <link rel="icon" type="image/x-icon" href="~/Content/img/favicon.ico" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v5.13.0/js/all.js" crossorigin="anonymous"></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Saira+Extra+Condensed:500,700" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Muli:400,400i,800,800i" rel="stylesheet" type="text/css" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/Content/css/styles.css" rel="stylesheet" />
    <!-- Custom CSS -->
    <link href="~/Content/css/custom.css" rel="stylesheet" />
</head>
<body id="page-top">
    <!-- Navigation -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top" id="sideNav">
        <a class="navbar-brand js-scroll-trigger" href="#page-top">
            <span class="d-block d-lg-none">S. Tessa Toney</span>
            <span class="d-none d-lg-block"><img class="img-fluid img-profile rounded-circle mx-auto mb-2" src="~/Content/img/profile.jpg" alt="profile image" /></span>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Index", "Home")">Home</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Contact", "Home")">Contact</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Resume", "Home")">R&eacute;sum&eacute;</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Links", "Home")">Links</a></li>
                <li class="nav-item"><a class="nav-link js-scroll-trigger" href="@Url.Action("Projects", "Home")">Projects</a></li>
            </ul>
        </div>
    </nav>



    @RenderBody()



    <!-- Lightbox Container -->
    <!-- Dark background that will cover the entire screen -->
    <div id="lightbox-container">
        <p>Click anywhere to close</p>
        <!-- Lightbox Content for Image -->
        <!-- Container for our large image -->
        <div id="lightbox-content"></div>
    </div>

    <!-- Bootstrap core JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Core theme JS -->
    <script src="~/Contentjs/scripts.js"></script>
    <!-- Custom JavaScript -->
    <script src="~/Contentjs/custom.js"></script>
    <!-- Custom jQuery -->
    <script src="~/Contentjs/jQuery.js"></script>
</body>
</html>
